{
  "name": "passage",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/jest": "22.0.0",
    "@types/react": "^16.0.31",
    "@types/react-native": "^0.51.3",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "concurrently": "^3.5.1",
    "jest-expo": "23.0.0",
    "react-test-renderer": "16.0.0",
    "rimraf": "^2.6.2",
    "rn-nodeify": "github:tradle/rn-nodeify",
    "tslint": "^5.8.0",
    "typescript": "^2.8.0-dev.20180209"
  },
  "scripts": {
    "start": "react-native start",
    "android": "npm run tsc && react-native run-android",
    "ios": "react-native run-ios",
    "test": "node node_modules/jest/bin/jest.js --watch",
    "tsc": "tsc",
    "clean": "rimraf artifacts",
    "build": "npm run clean && npm run tsc --",
    "apk": "cd android && ./gradlew assembleRelease",
    "lint": "tslint src/**/*.ts",
    "housekeeping": "react-native link && react-native upgrade",
    "fix-rn-os": "cat node_modules/react-native-os/android/src/main/java/com/peel/react/RNOSModule.java | sed -e '31{/@Override/d;}' > RNOSModule.tmp.java && mv RNOSModule.tmp.java node_modules/react-native-os/android/src/main/java/com/peel/react/RNOSModule.java",
    "adb-uninstall": "adb uninstall com.passage",
    "preadb-install": "npm run apk && npm run adb-uninstall",
    "adb-install": "adb install ./android/app/build/outputs/apk/app-release.apk"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "assert": "^1.4.1",
    "buffer": "^5.0.8",
    "dns.js": "^1.0.1",
    "events": "^1.1.1",
    "keypair": "^1.0.1",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "react": "16.0.0",
    "react-native": "0.53.0",
    "react-native-device-info": "^0.15.1",
    "react-native-http": "github:tradle/react-native-http#834492d",
    "react-native-level-fs": "^3.0.0",
    "react-native-os": "^1.1.0",
    "react-native-qrcode": "^0.2.6",
    "react-native-tcp": "^3.3.0",
    "react-native-udp": "^2.3.1",
    "react-native-webrtc": "^1.58.3",
    "react-native-zeroconf": "github:m0g/react-native-zeroconf#53d7fa1",
    "readable-stream": "^1.0.33",
    "sha.js": "^2.4.9",
    "url": "^0.10.3",
    "util": "^0.10.3"
  },
  "react-native": {
    "zlib": "browserify-zlib",
    "console": "console-browserify",
    "constants": "constants-browserify",
    "crypto": "react-native-crypto",
    "dns": "dns.js",
    "net": "react-native-tcp",
    "domain": "domain-browser",
    "http": "react-native-http",
    "https": "https-browserify",
    "os": "react-native-os",
    "path": "path-browserify",
    "querystring": "querystring-es3",
    "fs": "react-native-level-fs",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "dgram": "react-native-udp",
    "stream": "stream-browserify",
    "timers": "timers-browserify",
    "tty": "tty-browserify",
    "vm": "vm-browserify",
    "tls": false
  },
  "browser": {
    "zlib": "browserify-zlib",
    "console": "console-browserify",
    "constants": "constants-browserify",
    "crypto": "react-native-crypto",
    "dns": "dns.js",
    "net": "react-native-tcp",
    "domain": "domain-browser",
    "http": "react-native-http",
    "https": "https-browserify",
    "os": "react-native-os",
    "path": "path-browserify",
    "querystring": "querystring-es3",
    "fs": "react-native-level-fs",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "dgram": "react-native-udp",
    "stream": "stream-browserify",
    "timers": "timers-browserify",
    "tty": "tty-browserify",
    "vm": "vm-browserify",
    "tls": false
  }
}
